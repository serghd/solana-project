{
   "license": "ISC",
   "scripts": {
      "build": "anchor build",
      "deploy": "anchor build && solana program deploy --max-len 500000 target/deploy/solana_project.so",
      "initialize": "ts-node ./scripts/initialize.ts",
      "add-value": "ts-node ./scripts/addValue.ts",
      "get-value": "ts-node ./scripts/getValue.ts",
      "update-value": "ts-node ./scripts/updateValue.ts",
      "reset-value": "ts-node ./scripts/resetValue.ts",
      "update-security-txt": "ts-node ./scripts/updateSecurityTxt.ts",
      "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
      "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
      "format": "prettier --config ./.prettierrc.yml --write \"**/*.{js,json,md,sol,ts,yml}\" && cargo fmt"
   },
   "dependencies": {
      "@coral-xyz/anchor": "0.31.1",
      "dotenv": "17.2.1"
   },
   "devDependencies": {
      "@types/bn.js": "5.2.0",
      "@types/chai": "4.3.20",
      "@types/mocha": "9.1.1",
      "chai": "4.5.0",
      "mocha": "9.2.2",
      "prettier": "2.8.8",
      "ts-mocha": "10.1.0",
      "ts-node": "7.0.1",
      "typescript": "5.8.3"
   }
}
