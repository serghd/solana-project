{
   "name": "solana-project",
   "version": "0.0.1",
   "description": "",
   "license": "MIT",
   "scripts": {
      "build-program1": "anchor build --program-name program1",
      "build-program2": "anchor build --program-name program2",
      "deploy-program1": "solana program deploy --max-len 1000000 ./target/deploy/program1.so --program-id ./keypairs/program-keypair1.json",
      "deploy-program2": "solana program deploy --max-len 1000000 ./target/deploy/program2.so --program-id ./keypairs/program-keypair2.json",
      "initialize-program1": "ts-node ./scripts/program1Initialize.ts",
      "program1-add-value": "ts-node ./scripts/program1AddValue.ts",
      "program1-update-value": "ts-node ./scripts/program1UpdateValue.ts",
      "program1-get-value": "ts-node ./scripts/program1GetValue.ts",
      "program1-create-token": "ts-node ./scripts/program1CreateToken.ts",
      "program1-create-ata-account": "ts-node ./scripts/program1CreateAtaAccount.ts",
      "program1-mint-tokens": "ts-node ./scripts/program1MintTokens.ts",
      "program1-transfer-tokens": "ts-node ./scripts/program1TransferTokens.ts",
      "program2-call-transfer-tokens": "ts-node ./scripts/program2CallTransferTokens.ts",
      "program1-build-merkle-tree": "ts-node ./scripts/merkle-tree/program1BuildMerkleTree.ts",
      "program1-create-epoch": "ts-node ./scripts/merkle-tree/program1CreateEpoch.ts",
      "program1-verify-merkle-tree": "ts-node ./scripts/merkle-tree/program1VerifyMerkleTree.ts",
      "get-transaction": "ts-node ./scripts/getTransaction.ts",
      "test": "anchor test",
      "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
      "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
      "format": "prettier --config ./.prettierrc.yml --write \"**/*.{js,json,md,sol,ts,yml}\" && cargo fmt",
      "clean": "anchor clean"
   },
   "dependencies": {
      "@coral-xyz/anchor": "0.32.0",
      "@ethersproject/abi": "5.8.0",
      "@solana/spl-token": "0.4.14",
      "csv-parse": "6.1.0",
      "dotenv": "17.2.1",
      "keccak256": "1.0.6",
      "merkletreejs": "0.2.0"
   },
   "devDependencies": {
      "@types/bn.js": "5.2.0",
      "@types/chai": "5.2.2",
      "@types/mocha": "10.0.10",
      "chai": "6.2.0",
      "mocha": "11.7.4",
      "prettier": "2.8.8",
      "ts-mocha": "11.1.0",
      "ts-node": "10.9.2",
      "typescript": "5.8.3"
   }
}
