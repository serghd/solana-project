{
   "name": "solana-project",
   "version": "0.0.1",
   "description": "",
   "license": "MIT",
   "scripts": {
      "program1-build": "anchor build --program-name program1",
      "program2-build": "anchor build --program-name program2",
      "program1-deploy": "solana program deploy --max-len 1000000 ./target/deploy/program1.so --program-id ./keypairs/program-keypair1.json",
      "program2-deploy": "solana program deploy --max-len 1000000 ./target/deploy/program2.so --program-id ./keypairs/program-keypair2.json",
      "program1-initialize": "ts-node ./scripts/program1Initialize.ts",
      "program1-add-value": "ts-node ./scripts/program1AddValue.ts",
      "program1-update-value": "ts-node ./scripts/program1UpdateValue.ts",
      "program1-get-value": "ts-node ./scripts/program1GetValue.ts",
      "program1-create-token": "ts-node ./scripts/program1CreateToken.ts",
      "program1-create-ata-account": "ts-node ./scripts/program1CreateAtaAccount.ts",
      "program1-mint-tokens": "ts-node ./scripts/program1MintTokens.ts",
      "program1-transfer-tokens": "ts-node ./scripts/program1TransferTokens.ts",
      "program2-call-transfer-tokens": "ts-node ./scripts/program2CallTransferTokens.ts",
      "transaction-logs": "ts-node ./scripts/transactionLogs.ts",
      "test": "anchor test",
      "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
      "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
      "format": "prettier --config ./.prettierrc.yml --write \"**/*.{js,json,md,sol,ts,yml}\" && cargo fmt"
   },
   "dependencies": {
      "@coral-xyz/anchor": "0.31.1",
      "@solana/spl-token": "0.4.14",
      "dotenv": "17.2.1"
   },
   "devDependencies": {
      "@types/bn.js": "5.2.0",
      "@types/chai": "5.2.2",
      "@types/mocha": "10.0.10",
      "chai": "6.2.0",
      "mocha": "11.7.4",
      "prettier": "2.8.8",
      "ts-mocha": "11.1.0",
      "ts-node": "10.9.2",
      "typescript": "5.8.3"
   }
}
